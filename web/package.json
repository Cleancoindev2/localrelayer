{
  "name": "instex-web",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "test": "NODE_ENV=testing jest --env=jsdom -u",
    "test:watch": "yarn test --watchAll",
    "lint": "eslint ./src --ext .js,.jsx --ignore-pattern '/dist/' --ignore-pattern '*.prod.js' --ignore-pattern '*.test.js' --ignore-pattern '**/node_modules/*'",
    "start": "NODE_ENV=development node --trace-warnings ./node_modules/.bin/webpack-dev-server --config webpack.config.development.js",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.production.js --progress --profile --colors",
    "flow": "flow",
    "postinstall": "flow-typed install --overwrite && yarn link \"instex-core\"",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build"
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
      "^.+\\.less$": "<rootDir>/config/jest/cssTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules(?!/instex-core)[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupTests.js"
  },
  "dependencies": {
    "antd": "3.1.1",
    "bignumber.js": "5.0.0",
    "change-case": "^3.0.1",
    "file-loader": "^1.1.9",
    "history": "4.7.2",
    "moment": "2.20.1",
    "ramda": "0.25.0",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-grid-layout": "^0.16.6",
    "react-helmet": "^5.2.0",
    "react-joyride": "^1.11.4",
    "react-redux": "5.0.6",
    "react-responsive": "^4.1.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-scroll": "^1.7.6",
    "recompose": "0.26.0",
    "redux": "3.7.2",
    "redux-form": "7.2.0",
    "redux-logger": "3.0.6",
    "redux-resource": "2.4.0",
    "redux-resource-action-creators": "1.0.0",
    "redux-resource-plugins": "2.1.0",
    "redux-saga": "0.16.0",
    "regenerator-runtime": "^0.11.1",
    "reselect": "3.0.1",
    "styled-components": "2.4.0",
    "svg-inline-loader": "^0.8.0"
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-eslint": "8.1.1",
    "babel-jest": "22.0.4",
    "babel-loader": "7.1.2",
    "babel-plugin-import": "1.6.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "cross-env": "5.0.1",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.0",
    "eslint": "4.15.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-plugin-flowtype": "2.41.0",
    "eslint-plugin-flowtype-errors": "3.3.7-0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jest": "21.6.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.5.1",
    "eslint-import-resolver-webpack": "0.8.4",
    "extract-text-webpack-plugin": "3.0.2",
    "flow-bin": "0.63.1",
    "flow-typed": "2.2.3",
    "html-webpack-plugin": "2.30.1",
    "jest": "22.0.4",
    "jest-styled-components": "4.9.0",
    "less": "2.7.3",
    "less-loader": "4.0.5",
    "react-hot-loader": "3.1.3",
    "react-styleguidist": "6.1.0",
    "redux-devtools": "3.4.0",
    "redux-devtools-extension": "^2.13.2",
    "uglifyjs-webpack-plugin": "1.2.4",
    "webpack": "3.10.0",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-dev-server": "2.9.5",
    "webpack-merge": "4.1.1",
    "webpack-s3-plugin": "1.0.0-rc.0"
  }
}
