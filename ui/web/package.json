{
  "name": "web",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "test": "NODE_ENV=testing jest --env=jsdom -u",
    "test:watch": "yarn test --watchAll",
    "lint": "eslint ./src --ext .js,.jsx --ignore-pattern '/dist/' --ignore-pattern '*.prod.js' --ignore-pattern '*.test.js' --ignore-pattern '**/node_modules/*'",
    "start": "webpack-dev-server --open --color --env.NODE_ENV=development --config webpack.config.development.js",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.production.js --progress --profile --colors",
    "flow": "flow",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build"
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
      "^.+\\.less$": "<rootDir>/config/jest/cssTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules(?!/instex-core)[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupTests.js"
  },
  "dependencies": {
    "@babel/polyfill": "7.0.0-rc.1",
    "@babel/runtime": "7.0.0-rc.1",
    "antd": "3.8.2",
    "bignumber.js": "5.0.0",
    "change-case": "3.0.2",
    "classnames": "2.2.6",
    "history": "4.7.2",
    "instex-core": "0.0.1",
    "moment": "2.20.1",
    "ramda": "0.25.0",
    "raven-js": "3.26.4",
    "react": "16.4.2",
    "react-animations": "1.0.0",
    "react-dom": "16.4.2",
    "react-grid-layout": "0.16.6",
    "react-helmet": "5.2.0",
    "react-hot-loader": "4.3.4",
    "react-joyride": "1.11.4",
    "react-redux": "5.0.7",
    "react-responsive": "5.0.0",
    "react-router-dom": "4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-select": "2.0.0",
    "recompose": "0.28.2",
    "redux-form": "7.4.2",
    "redux-logger": "3.0.6",
    "reselect": "3.0.1",
    "scriptjs": "2.5.8",
    "styled-components": "3.4.2"
  },
  "devDependencies": {
    "@babel/core": "7.0.0-rc.1",
    "@babel/plugin-external-helpers": "7.0.0-rc.1",
    "@babel/plugin-proposal-class-properties": "7.0.0-rc.1",
    "@babel/plugin-proposal-do-expressions": "7.0.0-rc.1",
    "@babel/plugin-proposal-export-default-from": "7.0.0-rc.1",
    "@babel/plugin-proposal-export-namespace-from": "7.0.0-rc.1",
    "@babel/plugin-proposal-optional-chaining": "7.0.0-rc.1",
    "@babel/plugin-transform-runtime": "7.0.0-rc.1",
    "@babel/preset-env": "7.0.0-rc.1",
    "@babel/preset-flow": "7.0.0-rc.1",
    "@babel/preset-react": "7.0.0-rc.1",
    "babel-loader": "8.0.0-beta.4",
    "babel-plugin-import": "1.8.0",
    "babel-plugin-styled-components": "1.5.1",
    "css-loader": "1.0.0",
    "file-loader": "1.1.11",
    "html-webpack-plugin": "3.2.0",
    "js-to-styles-var-loader": "^1.1.0",
    "less": "3.8.1",
    "less-loader": "4.1.0",
    "less-plugin-lists": "1.1.2",
    "style-loader": "0.22.1",
    "webpack": "4.17.0",
    "webpack-cli": "3.1.0",
    "webpack-dev-server": "3.1.5",
    "webpack-merge": "4.1.4"
  }
}
