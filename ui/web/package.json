{
  "name": "web",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "test": "jest --no-cache",
    "test:watch": "yarn test --watchAll",
    "lint": "eslint ./src --ext .js,.jsx --ignore-pattern '/dist/' --ignore-pattern '*.prod.js' --ignore-pattern '*.test.js' --ignore-pattern '**/node_modules/*'",
    "start": "webpack-dev-server --open --color --env.NODE_ENV=development --config webpack.config.development.js",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.production.js --progress --profile --colors",
    "flow": "flow",
    "storybook": "start-storybook -p 9001 -c .storybook -s ./dist"
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "<rootDir>/babel-jest-transform.js"
    },
    "moduleNameMapper": {
      "web-components(.*)$": "<rootDir>/src/components/$1"
    }
  },
  "dependencies": {
    "@babel/polyfill": "7.0.0",
    "@babel/runtime": "7.0.0",
    "antd": "3.9.3",
    "change-case": "3.0.2",
    "classnames": "2.2.6",
    "formik": "1.3.2",
    "history": "4.7.2",
    "instex-core": "0.0.1",
    "raven-js": "3.26.4",
    "react": "16.7.0-alpha.2",
    "react-animations": "1.0.0",
    "react-dom": "16.7.0-alpha.2",
    "react-grid-layout": "0.16.6",
    "react-helmet": "5.2.0",
    "react-hot-loader": "4.3.12",
    "react-joyride": "1.11.4",
    "react-redux": "5.0.7",
    "react-responsive": "5.0.0",
    "react-router-dom": "4.3.1",
    "react-select": "2.0.0",
    "react-test-renderer": "16.4.2",
    "recompose": "0.30.0",
    "redux-form": "7.4.2",
    "redux-logger": "3.0.6",
    "reselect": "3.0.1",
    "scriptjs": "2.5.8",
    "styled-components": "3.4.5"
  },
  "devDependencies": {
    "@storybook/addon-actions": "4.0.2",
    "@storybook/addon-info": "4.0.2",
    "@storybook/addon-knobs": "4.0.2",
    "@storybook/addon-options": "4.0.2",
    "@storybook/addon-viewport": "4.0.2",
    "@storybook/react": "4.0.2",
    "babel-loader": "8.0.2",
    "babel-plugin-import": "1.8.0",
    "babel-plugin-styled-components": "1.6.0",
    "css-loader": "1.0.0",
    "file-loader": "2.0.0",
    "html-webpack-plugin": "3.2.0",
    "js-to-styles-var-loader": "1.1.0",
    "less": "3.8.1",
    "less-loader": "4.1.0",
    "less-plugin-lists": "1.1.2",
    "react-json-view": "1.19.1",
    "style-loader": "0.23.0",
    "webpack": "4.17.1",
    "webpack-cli": "3.1.0",
    "webpack-dev-server": "3.1.7",
    "webpack-merge": "4.1.4"
  }
}
