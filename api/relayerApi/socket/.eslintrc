{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "browser": true,
    "mocha": true,
    "node": true
  },
  "rules": {
    "comma-dangle": 0,
    "no-param-reassign": 0,
    "func-names": 0,
    "no-console": 0,
    "no-shadow": 0,
    "no-use-before-define": 0,
    "consistent-return": 0
  }
}
