{
  "name": "instex-api",
  "version": "0.0.1",
  "description": "Instex api server",
  "main": "index.js",
  "scripts": {
    "start-dev": "concurrently \"yarn dev\" \"yarn matcher-queue\" \"yarn fill-queue\" \"cd socket && yarn dev\"",
    "testnet": "ganache-cli --networkId 50 -p 8545 --db ./testrpc_snapshot -m 'concert load couple harbor equip island argue ramp clarify fence smart topic'",
    "lint": "eslint src *.js",
    "docs": "./node_modules/.bin/apidoc -i src/ -o docs",
    "dev": "cross-env NODE_ENV=development babel-watch src/index.js",
    "clean": "rm -rf build && mkdir build",
    "start": "node ./dist/index.js",
    "dev-test-queue": "cross-env NODE_ENV=development babel-watch src/queue/example.js",
    "matcher-queue": "cross-env NODE_ENV=development babel-watch src/queue/matcherQueue.js",
    "build-matcher-queue": "cross-env NODE_ENV=production npm run clean && ./node_modules/babel-cli/bin/babel.js src/queue/matcherQueue.js -d matcherQueueDist",
    "update-queue": "cross-env NODE_ENV=development babel-watch src/queue/updateQueue.js",
    "build-update-queue": "cross-env NODE_ENV=production npm run clean && ./node_modules/babel-cli/bin/babel.js src/queue/updateQueue.js -d fillQueueDist",
    "build": "cross-env NODE_ENV=production npm run clean && ./node_modules/babel-cli/bin/babel.js src -d dist",
    "knex": "babel-node node_modules/.bin/knex",
    "initializeDevDB": "cross-env NODE_ENV=development babel-node src/commands/initializeDB.js",
    "initializeProdDB": "cross-env NODE_ENV=production babel-node src/commands/initializeDB.js",
    "initializeTestDB": "cross-env NODE_ENV=test babel-node src/commands/initializeDB.js",
    "plantDevFixtures": "cross-env NODE_ENV=development babel-node fixtures/index.js",
    "plantProdFixtures": "cross-env NODE_ENV=production babel-node fixtures/index.js",
    "plantTestFixtures": "cross-env NODE_ENV=test babel-node fixtures/index.js",
    "test": "cross-env NODE_ENV=test mocha test/**/** --require babel-register",
    "test:orders": "cross-env NODE_ENV=test mocha --watch test/orders/* --require babel-register",
    "test:tokens": "cross-env NODE_ENV=test mocha --watch test/tokens/* --require babel-register",
    "test:match": "cross-env NODE_ENV=test mocha --timeout 15000 test/matcher.test.js --require babel-register",
    "processFailedOrders": "cross-env NODE_ENV=production babel-node src/commands/processFailedOrders.js",
    "processNewOrders": "cross-env NODE_ENV=production babel-node src/commands/processNewOrders.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:web-pal/instex-api.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "homepage": "https://github.com/web-pal/instex-api",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-register": "^6.24.1",
    "babel-watch": "^2.0.7",
    "chai": "^4.1.1",
    "cross-env": "^5.0.5",
    "eslint": "^4.4.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.1.0",
    "mocha": "^3.5.0",
    "readline-sync": "^1.4.7",
    "supertest": "^3.0.0"
  },
  "dependencies": {
    "0x.js": "0.34.0",
    "@0xproject/web3-wrapper": "0.7.0",
    "bcrypt": "^2.0.1",
    "bignumber.js": "^5.0.0",
    "bluebird": "^3.5.0",
    "bookshelf": "^0.10.4",
    "bookshelf-jsonapi-params": "git://github.com/web-pal/bookshelf-jsonapi-params.git#master",
    "bookshelf-page": "^0.3.2",
    "bookshelf-paranoia": "^0.11.0",
    "bookshelf-postgis": "^0.2.2",
    "bookshelf-uuid": "^1.0.0",
    "co-redis": "^2.1.1",
    "concurrently": "^3.5.1",
    "debug": "^3.0.0",
    "expo-server-sdk": "^2.3.3",
    "faker": "git://github.com/Marak/faker.js.git",
    "fortune": "^5.2.11",
    "fortune-postgres": "^1.6.3",
    "ganache-cli": "^6.1.0",
    "import-dir": "^0.0.1",
    "inflection": "^1.12.0",
    "jsonapi-mapper": "^1.0.0-beta.13",
    "jsonapi-query-parser": "git://github.com/VladimirPal/node-jsonapi-query-parser.git",
    "jsonwebtoken": "^7.4.2",
    "kcors": "^2.2.1",
    "knex": "^0.13.0",
    "koa": "2.3.0",
    "koa-bodyparser": "^4.2.0",
    "koa-compose": "^4.0.0",
    "koa-logger": "^3.0.1",
    "koa-mount": "^3.0.0",
    "koa-passport": "^3.0.0",
    "koa-router": "^7.2.1",
    "koa-static": "^4.0.1",
    "kue": "0.11.6",
    "lodash": "^4.17.4",
    "mathjs": "^3.16.3",
    "moment": "^2.20.1",
    "node-fetch": "^1.7.2",
    "nodemon": "^1.14.12",
    "normalizr": "^3.2.3",
    "passport-jwt": "^2.2.1",
    "passport-strategy": "^1.0.0",
    "pg": "^7.1.0",
    "pg-hstore": "^2.3.2",
    "raven": "2.6.0",
    "redis": "2.8.0",
    "sequelize": "^4.7.5",
    "socket.io-emitter": "3.1.1",
    "split-string": "^3.0.2",
    "sql-fixtures": "^1.0.0",
    "swagger-jsdoc": "^1.9.7",
    "telegraf": "^3.20.0",
    "uuid": "^3.2.1",
    "validate.js": "^0.11.1",
    "web3": "1.0.0-beta.33",
    "websocket": "^1.0.26",
    "winston": "^3.0.0-rc1",
    "wkx": "^0.4.2"
  }
}
